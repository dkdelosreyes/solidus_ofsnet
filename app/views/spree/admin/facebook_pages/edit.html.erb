<%= render partial: 'spree/shared/error_messages', locals: { target: @facebook_page } %>

<% admin_breadcrumb(@facebook_page.name) %>

<%= form_for [:admin, @facebook_page], html: { multipart: true } do |f| %>
  <fieldset class="no-border-top">
    <%= render partial: 'form', locals: { f: f } %>
    <% if can?(:update, @facebook_page) %>
      <%= render partial: 'spree/admin/shared/edit_resource_links' %>
    <% end %>
  </fieldset>
<% end %>